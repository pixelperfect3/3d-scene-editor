	</WidgetLook>


	<!--
	***************************************************
		RevoltLook/ComboDropList
	***************************************************
	-->
	<WidgetLook name="RevoltLook/ComboDropList">
		<NamedArea name="ItemRenderingArea">
			<Area>
				<Dim type="LeftEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameLeft" dimension="Width" /></Dim>
				<Dim type="TopEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameTop" dimension="Height" /></Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="StaticFrameRight" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge" >
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="StaticFrameBottom" dimension="Height" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="ItemRenderingAreaHScroll">
			<Area>
				<Dim type="LeftEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameLeft" dimension="Width" /></Dim>
				<Dim type="TopEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameTop" dimension="Height" /></Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="StaticFrameRight" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge" >
					<UnifiedDim scale="1" offset="-16" type="BottomEdge" />
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="ItemRenderingAreaVScroll">
			<Area>
				<Dim type="LeftEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameLeft" dimension="Width" /></Dim>
				<Dim type="TopEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameTop" dimension="Height" /></Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" offset="-16" type="RightEdge" />
				</Dim>
				<Dim type="BottomEdge" >
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="StaticFrameBottom" dimension="Height" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="ItemRenderingAreaHVScroll">
			<Area>
				<Dim type="LeftEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameLeft" dimension="Width" /></Dim>
				<Dim type="TopEdge" ><ImageDim imageset="RevoltLook" image="StaticFrameTop" dimension="Height" /></Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" offset="-16" type="RightEdge" />
				</Dim>
				<Dim type="BottomEdge" >
					<UnifiedDim scale="1" offset="-16" type="BottomEdge" />
				</Dim>
			</Area>
		</NamedArea>
		<Child  type="RevoltLook/HorizontalScrollbar" nameSuffix="__auto_hscrollbar__">
			<Area>
				<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="Width" ><UnifiedDim scale="1" offset="-16" type="Width" /></Dim>
				<Dim type="Height" ><AbsoluteDim value="16" /></Dim>
			</Area>
			<VertAlignment type="BottomAligned" />
		</Child>
		<Child  type="RevoltLook/VerticalScrollbar" nameSuffix="__auto_vscrollbar__">
			<Area>
				<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="Width" ><AbsoluteDim value="16" /></Dim>
				<Dim type="Height" ><UnifiedDim scale="1" offset="-16" type="Height" /></Dim>
			</Area>
			<HorzAlignment type="RightAligned" />
		</Child>
		<ImagerySection name="main">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
					<Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
				</Area>
				<Image type="TopLeftCorner" imageset="RevoltLook" image="StaticFrameTopLeft" />
				<Image type="TopRightCorner" imageset="RevoltLook" image="StaticFrameTopRight" />
				<Image type="BottomLeftCorner" imageset="RevoltLook" image="StaticFrameBottomLeft" />
				<Image type="BottomRightCorner" imageset="RevoltLook" image="StaticFrameBottomRight" />
				<Image type="LeftEdge" imageset="RevoltLook" image="StaticFrameLeft" />
				<Image type="RightEdge" imageset="RevoltLook" image="StaticFrameRight" />
				<Image type="TopEdge" imageset="RevoltLook" image="StaticFrameTop" />
				<Image type="BottomEdge" imageset="RevoltLook" image="StaticFrameBottom" />
				<Image type="Background" imageset="RevoltLook" image="Background" />
			</FrameComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="main" />
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="main" />
			</Layer>
		</StateImagery>
	</WidgetLook>


	<!--
	***************************************************
		RevoltLook/Combobox
	***************************************************
	-->
	<WidgetLook name="RevoltLook/Combobox">
		<Child  type="RevoltLook/Editbox" nameSuffix="__auto_editbox__">
			<Area>
				<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="FrameTop" dimension="Height">
								<DimOperator op="Add">
									<ImageDim imageset="RevoltLook" image="FrameBottom" dimension="Height">
										<DimOperator op="Add">
											<FontDim type="LineSpacing">
												<DimOperator op="Multiply">
													<AbsoluteDim value="1.5" />
												</DimOperator>
											</FontDim>
										</DimOperator>
									</ImageDim>
								</DimOperator>
							</ImageDim>
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="Height" >
					<ImageDim imageset="RevoltLook" image="FrameTop" dimension="Height">
						<DimOperator op="Add">
							<ImageDim imageset="RevoltLook" image="FrameBottom" dimension="Height">
								<DimOperator op="Add">
									<FontDim type="LineSpacing">
										<DimOperator op="Multiply">
											<AbsoluteDim value="1.5" />
										</DimOperator>
									</FontDim>
								</DimOperator>
							</ImageDim>
						</DimOperator>
					</ImageDim>
				</Dim>
			</Area>
		</Child>
		<Child  type="RevoltLook/ComboDropList" nameSuffix="__auto_droplist__">
			<Area>
				<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="TopEdge" ><WidgetDim widget="__auto_editbox__" dimension="BottomEdge" /></Dim>
				<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
				<Dim type="BottomEdge" ><UnifiedDim scale="1" type="BottomEdge" /></Dim>
			</Area>
		</Child>
		<Child  type="RevoltLook/IconButton" nameSuffix="__auto_button__">
			<Area>
				<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
				<Dim type="Width" ><WidgetDim widget="__auto_editbox__" dimension="Height" /></Dim>
				<Dim type="Height" ><WidgetDim widget="__auto_editbox__" dimension="Height" /></Dim>
			</Area>
			<HorzAlignment type="RightAligned" />
			<Property name="IconImage" value="set:RevoltLook image:LargeDownArrow" />
		</Child>
		<StateImagery name="Enabled" />
		<StateImagery name="Disabled" />
	</WidgetLook>


	<!--
	***************************************************
		RevoltLook/Editbox
	***************************************************
	-->
	<WidgetLook name="RevoltLook/Editbox">
		<PropertyDefinition name="ReadOnlyBGColour" initialValue="FF000000" />
		<Property name="MouseCursorImage" value="set:RevoltLook image:MouseTextBar" />
		<Property name="NormalTextColour" value="FFFFFFFF" />
		<Property name="SelectedTextColour" value="FF000000" />
		<Property name="ActiveSelectionColour" value="FFAABBBB" />
		<Property name="InactiveSelectionColour" value="FF667777" />
		<NamedArea name="TextArea">
			<Area>
				<Dim type="LeftEdge">
					<ImageDim imageset="RevoltLook" image="FrameLeft" dimension="Width" />
				</Dim>
				<Dim type="TopEdge">
					<ImageDim imageset="RevoltLook" image="FrameTop" dimension="Height">
						<DimOperator op="Subtract">
							<AbsoluteDim value="3" />
						</DimOperator>					
					</ImageDim>
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="FrameRight" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="RevoltLook" image="FrameBottom" dimension="Height">
								<DimOperator op="Subtract">
									<AbsoluteDim value="1" />
								</DimOperator>
							</ImageDim>
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<ImagerySection name="container_normal">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width" >
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height" >
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="TopLeftCorner" imageset="RevoltLook" image="FrameTopLeft" />
				<Image type="TopRightCorner" imageset="RevoltLook" image="FrameTopRight" />
				<Image type="BottomLeftCorner" imageset="RevoltLook" image="FrameBottomLeft" />
				<Image type="BottomRightCorner" imageset="RevoltLook" image="FrameBottomRight" />
				<Image type="LeftEdge" imageset="RevoltLook" image="FrameLeft" />
				<Image type="TopEdge" imageset="RevoltLook" image="FrameTop" />
				<Image type="RightEdge" imageset="RevoltLook" image="FrameRight" />
				<Image type="BottomEdge" imageset="RevoltLook" image="FrameBottom" />
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="selection">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge" >
						<AbsoluteDim value="2" />
					</Dim>
					<Dim type="RightEdge" >
						<UnifiedDim scale="1.0" type="RightEdge" />
					</Dim>
					<Dim type="BottomEdge" >
						<UnifiedDim scale="1.0" type="BottomEdge" />
					</Dim>
				</Area>
				<Image imageset="RevoltLook" image="Background" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="Carat">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="2" />
					</Dim>
					<Dim type="Width">
						<ImageDim imageset="RevoltLook" image="EditBoxCarat" dimension="Width" />
					</Dim>
					<Dim type="BottomEdge" >
						<UnifiedDim scale="1.0" type="BottomEdge" />
					</Dim>
				</Area>
				<Image imageset="RevoltLook" image="EditBoxCarat" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="container_normal">
					<Colours topLeft="FFCCCCCC" topRight="FFCCCCCC" bottomLeft="FFCCCCCC" bottomRight="FFCCCCCC" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="ReadOnly">
			<Layer>
				<Section section="container_normal">
					<ColourProperty name="ReadOnlyBGColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="container_normal">
					<ColourProperty name="ReadOnlyBGColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="ActiveSelection">
			<Layer>
				<Section section="selection">
					<ColourProperty name="ActiveSelectionColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="InactiveSelection">
			<Layer>
				<Section section="selection">
					<ColourProperty name="InactiveSelectionColour" />
