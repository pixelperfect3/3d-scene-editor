				<Section section="normal">
					<Colours topLeft="FF333333" topRight="FF333333" bottomLeft="FF333333" bottomRight="FF333333" />
				</Section>
				<Section section="caption" />
			</Layer>
		</StateImagery>
	</WidgetLook>


