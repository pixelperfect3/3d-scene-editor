<?xml version="1.0" ?>
<Falagard>
	<!--
	***************************************************
		RevoltLook/Button
	***************************************************
	-->
	<WidgetLook name="RevoltLook/Button">
		<Property name="NormalTextColour" value="FFEEEEEE" />
		<Property name="HoverTextColour" value="FFFFFFFF" />
		<Property name="PushedTextColour" value="FFEEFFEE" />
		<Property name="DisabledTextColour" value="FF999999" />
		<ImagerySection name="label">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="2" />
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" type="RightEdge" />
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" type="BottomEdge">
							<DimOperator op="Subtract">
								<ImageDim imageset="RevoltLook" image="FrameBottom" dimension="Height" />
							</DimOperator>
						</UnifiedDim>
					</Dim>
				</Area>
				<VertFormat type="CentreAligned" />
				<HorzFormat type="WordWrapCentreAligned" />
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="button">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" type="RightEdge" />
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" type="BottomEdge" />
					</Dim>
				</Area>
				<Image type="TopLeftCorner" imageset="RevoltLook" image="FrameTopLeft" />
				<Image type="TopRightCorner" imageset="RevoltLook" image="FrameTopRight" />
				<Image type="BottomLeftCorner" imageset="RevoltLook" image="FrameBottomLeft" />
				<Image type="BottomRightCorner" imageset="RevoltLook" image="FrameBottomRight" />
				<Image type="LeftEdge" imageset="RevoltLook" image="FrameLeft" />
				<Image type="TopEdge" imageset="RevoltLook" image="FrameTop" />
				<Image type="RightEdge" imageset="RevoltLook" image="FrameRight" />
				<Image type="BottomEdge" imageset="RevoltLook" image="FrameBottom" />
			</FrameComponent>
		</ImagerySection>
		<StateImagery name="Normal">
			<Layer>
				<Section section="button">
					<Colours topLeft="FFCCCCCC" topRight="FFCCCCCC" bottomLeft="FFCCCCCC" bottomRight="FFCCCCCC" />
				</Section>
				<Section section="label">
					<ColourProperty name="NormalTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Hover">
			<Layer>
				<Section section="button">
					<Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
				</Section>
				<Section section="label">
					<ColourProperty name="HoverTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Pushed">
			<Layer>
				<Section section="button">
					<Colours topLeft="FF66FF66" topRight="FF66FF66" bottomLeft="FF66FF66" bottomRight="FF66FF66" />
				</Section>
				<Section section="label">
					<ColourProperty name="PushedTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="button">
					<Colours topLeft="FF333333" topRight="FF333333" bottomLeft="FF333333" bottomRight="FF333333" />
				</Section>
				<Section section="label">
					<ColourProperty name="DisabledTextColour" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>


	<!--
	***************************************************
		RevoltLook/Checkbox
	***************************************************
	-->
	<WidgetLook name="RevoltLook/Checkbox">
		<Property name="NormalTextColour" value="FF000000" />
		<Property name="HoverTextColour" value="FF000000" />
		<Property name="PushedTextColour" value="FF000000" />
		<Property name="DisabledTextColour" value="FF888888" />
		<ImagerySection name="label">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge" >
						<ImageDim imageset="RevoltLook" image="CheckboxNormal" dimension="Width">
							<DimOperator op="Add">
								<AbsoluteDim value="4" />
							</DimOperator>
						</ImageDim>
					</Dim>
					<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="RightEdge" ><UnifiedDim scale="1" type="Width" /></Dim>
					<Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
				</Area>
				<VertFormat type="CentreAligned" />
				<HorzFormat type="LeftAligned" />
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="normal">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
					<Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
				</Area>
				<Image imageset="RevoltLook" image="CheckboxNormal" />
				<VertFormat type="CentreAligned" />
				<HorzFormat type="LeftAligned" />
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="hover">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
					<Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
				</Area>
				<Image imageset="RevoltLook" image="CheckboxHover" />
				<VertFormat type="CentreAligned" />
				<HorzFormat type="LeftAligned" />
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="select_mark">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
					<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
					<Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
				</Area>
				<Image imageset="RevoltLook" image="CheckboxMark" />
				<VertFormat type="CentreAligned" />
				<HorzFormat type="LeftAligned" />
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="Normal">
			<Layer>
				<Section section="normal" />
				<Section section="label">
					<ColourProperty name="NormalTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Hover">
			<Layer>
				<Section section="hover" />
				<Section section="label">
					<ColourProperty name="HoverTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="SelectedNormal">
			<Layer>
				<Section section="normal" />
				<Section section="select_mark" />
				<Section section="label">
					<ColourProperty name="NormalTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="SelectedHover">
			<Layer>
				<Section section="hover" />
				<Section section="select_mark" />
				<Section section="label">
					<ColourProperty name="HoverTextColour" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>


